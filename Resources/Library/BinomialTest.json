{
	"version": "2.00",
	"autorun": true,
	"init": true,
	"name": "BinomialTest",
	"title": "Binomial Test",
	"dataset": {
		"factor": "variables"
	},
	"options": [
		{
			"name": "variables",
			"type": "Variables"
		},
		{
			"format": "",
			"name": "testValue",
			"type": "Number",
			"value": 0.5,
			"max": 1,
			"min": 0
		},
		{
			"name": "hypothesis",
			"options": ["notEqualToTestValue", "greaterThanTestValue", "lessThanTestValue"],
			"default": "notEqualToTestValue",
			"type": "List"
		},
		{
			"name": "confidenceInterval",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "confidenceIntervalInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "descriptivesPlots",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "descriptivesPlotsConfidenceInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "VovkSellkeMPR",
			"type": "Boolean",
			"default": false
		}
	],
    "results": [
        {
            "name": "binomial",
			"type": "table",
			"title": "Binomial Test",
			"columns": [
				{
					"name": "case",
					"title": "",
					"type": "string",
					"combine": true
				},
				{
					"name": "level",
					"title": "Level",
					"type": "string"
				},
				{
					"name": "counts",
					"title": "Counts",
					"type": "integer"
				},
				{
					"name": "total",
					"title": "Total",
					"type": "string"
				},
				{
					"name": "proportion",
					"title": "Proportion",
					"type": "number",
					"format": "sf:4;dp:3"
				},
				{
					"name": "p",
					"title": "p",
					"type": "number",
					"format": "dp:3;p:.001"
				},
				{
					"name": "VovkSellkeMPR",
					"title": "VS-MPR\u002A",
					"type": "number",
					"format": "sf:4;dp:3"
				},
				{
					"name": "lowerCI",
					"title": "Lower",
					"type": "number",
					"format": "sf:4;dp:3"
				},
				{
					"name": "upperCI",
					"title": "Upper",
					"type": "number",
					"format": "sf:4;dp:3"
				}
			]
		},
        {
            "name": "descriptives",
            "type": "container",
            "items": [{
                "descriptivesPlot": {
                    "name": "descriptivesPlot",
                    "type": "image",
                    "width": 160,
                    "height": 300
                }
            }]
        }
    ],
	"state": {
		"binomResults": ["variables", "confidenceIntervalInterval", "hypothesis", "testValue"],
		"descriptPlots": ["variables", "descriptivesPlots", "descriptivesPlotsConfidenceInterval", "plotWidth", "plotHeight"]
	}
}
